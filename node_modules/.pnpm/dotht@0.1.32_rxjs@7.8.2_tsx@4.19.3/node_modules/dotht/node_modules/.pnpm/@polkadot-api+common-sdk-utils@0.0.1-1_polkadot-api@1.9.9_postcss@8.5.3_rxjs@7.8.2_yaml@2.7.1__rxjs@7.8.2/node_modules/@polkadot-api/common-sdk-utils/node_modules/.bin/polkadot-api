#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/filippovecchiato/paritytech/test_phub/node_modules/.pnpm/dotht@0.1.32_rxjs@7.8.2_tsx@4.19.3/node_modules/dotht/node_modules/.pnpm/polkadot-api@1.9.9_postcss@8.5.3_rxjs@7.8.2_yaml@2.7.1/node_modules/polkadot-api/bin/node_modules:/Users/filippovecchiato/paritytech/test_phub/node_modules/.pnpm/dotht@0.1.32_rxjs@7.8.2_tsx@4.19.3/node_modules/dotht/node_modules/.pnpm/polkadot-api@1.9.9_postcss@8.5.3_rxjs@7.8.2_yaml@2.7.1/node_modules/polkadot-api/node_modules:/Users/filippovecchiato/paritytech/test_phub/node_modules/.pnpm/dotht@0.1.32_rxjs@7.8.2_tsx@4.19.3/node_modules/dotht/node_modules/.pnpm/polkadot-api@1.9.9_postcss@8.5.3_rxjs@7.8.2_yaml@2.7.1/node_modules:/Users/filippovecchiato/paritytech/test_phub/node_modules/.pnpm/dotht@0.1.32_rxjs@7.8.2_tsx@4.19.3/node_modules/dotht/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/filippovecchiato/paritytech/test_phub/node_modules/.pnpm/dotht@0.1.32_rxjs@7.8.2_tsx@4.19.3/node_modules/dotht/node_modules/.pnpm/polkadot-api@1.9.9_postcss@8.5.3_rxjs@7.8.2_yaml@2.7.1/node_modules/polkadot-api/bin/node_modules:/Users/filippovecchiato/paritytech/test_phub/node_modules/.pnpm/dotht@0.1.32_rxjs@7.8.2_tsx@4.19.3/node_modules/dotht/node_modules/.pnpm/polkadot-api@1.9.9_postcss@8.5.3_rxjs@7.8.2_yaml@2.7.1/node_modules/polkadot-api/node_modules:/Users/filippovecchiato/paritytech/test_phub/node_modules/.pnpm/dotht@0.1.32_rxjs@7.8.2_tsx@4.19.3/node_modules/dotht/node_modules/.pnpm/polkadot-api@1.9.9_postcss@8.5.3_rxjs@7.8.2_yaml@2.7.1/node_modules:/Users/filippovecchiato/paritytech/test_phub/node_modules/.pnpm/dotht@0.1.32_rxjs@7.8.2_tsx@4.19.3/node_modules/dotht/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../polkadot-api@1.9.9_postcss@8.5.3_rxjs@7.8.2_yaml@2.7.1/node_modules/polkadot-api/bin/cli.mjs" "$@"
else
  exec node  "$basedir/../../../../../../polkadot-api@1.9.9_postcss@8.5.3_rxjs@7.8.2_yaml@2.7.1/node_modules/polkadot-api/bin/cli.mjs" "$@"
fi
