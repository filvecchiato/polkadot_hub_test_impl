#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/filippovecchiato/paritytech/test_phub/node_modules/.pnpm/@polkadot-api+cli@0.11.6_tsx@4.19.3/node_modules/@polkadot-api/cli/dist/node_modules:/Users/filippovecchiato/paritytech/test_phub/node_modules/.pnpm/@polkadot-api+cli@0.11.6_tsx@4.19.3/node_modules/@polkadot-api/cli/node_modules:/Users/filippovecchiato/paritytech/test_phub/node_modules/.pnpm/@polkadot-api+cli@0.11.6_tsx@4.19.3/node_modules/@polkadot-api/node_modules:/Users/filippovecchiato/paritytech/test_phub/node_modules/.pnpm/@polkadot-api+cli@0.11.6_tsx@4.19.3/node_modules:/Users/filippovecchiato/paritytech/test_phub/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/filippovecchiato/paritytech/test_phub/node_modules/.pnpm/@polkadot-api+cli@0.11.6_tsx@4.19.3/node_modules/@polkadot-api/cli/dist/node_modules:/Users/filippovecchiato/paritytech/test_phub/node_modules/.pnpm/@polkadot-api+cli@0.11.6_tsx@4.19.3/node_modules/@polkadot-api/cli/node_modules:/Users/filippovecchiato/paritytech/test_phub/node_modules/.pnpm/@polkadot-api+cli@0.11.6_tsx@4.19.3/node_modules/@polkadot-api/node_modules:/Users/filippovecchiato/paritytech/test_phub/node_modules/.pnpm/@polkadot-api+cli@0.11.6_tsx@4.19.3/node_modules:/Users/filippovecchiato/paritytech/test_phub/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/main.js" "$@"
else
  exec node  "$basedir/../../dist/main.js" "$@"
fi
